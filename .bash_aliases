

# Easier navigation: .., ..., ...., ....., ~ 
alias ll='ls -ahlF'
alias la='ls -A'
alias l='ls -CF'


alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~" # `cd` is probably faster to type though
alias -- -="cd -"  # -- is often used (especially with Gnu programs) to stop parsing the following arguments as options. 

alias clr="clear"


alias gs="git status"
alias gb="git branch"

alias smi="nvidia-smi"


